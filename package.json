{
  "name": "dafei-www-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.dev -- tsx ./src/dev_server.ts",
    "check": "tsc",
    "build:app": "dotenv -e .env.prod -- remix vite:build --minify=esbuild",
    "build:ws": "esbuild ./src/ws --bundle --minify --platform=node --packages=external --outfile=./dist/ws.cjs",
    "build": "npm run build:app && npm run build:ws",
    "start": "dotenv -e .env.prod -- node ./server.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@remix-run/express": "^2.10.2",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "compression": "^1.7.4",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "isbot": "^5.1.12",
    "jssha": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "remix-i18next": "^6.1.0",
    "sirv": "^2.0.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.5.11",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.39",
    "postcss-preset-env": "^9.6.0",
    "prisma": "^5.16.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
